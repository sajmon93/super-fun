#!/bin/bash
page2="$1"
while  read -r line
 do 
 name=$line
 curl $line >> page3
 
#echo "name - $line"

done < page2

grep -i -E '<a href=".*">' page3 > page4
grep -E -o 'href="([^"#]+)' page4 | cut -d'"' -f2 > page5 
grep http page5  > page2

